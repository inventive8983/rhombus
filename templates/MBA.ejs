<!DOCTYPE html>
<html lang="en">
<%- include('components/head') %>
    <%- include('components/theme') %>
        <%- include('components/header') %>

            <div class="content-section bg-default-1 border-top py-13">
                <div class="container">
                    <div class="row align-items-center justify-content-center">
                        <div class="col-sm-10 col-lg-6 col-xl-6 offset-xl-1  order-lg-2 mb-9 mb-lg-0">
                            <div class="double-image-group-opacity" data-aos="zoom-in" data-aos-duration="1200"
                                data-aos-once="true">
                                <img class="main-img w-100" src="/static/image/svg/mba.svg" alt="" />
                            </div>
                        </div>
                        <div class="col-sm-10 col-lg-5 col-xl-5 order-lg-1">
                            <div class="content-text">
                                <h2 class="gr-text-4 mb-7">We want to make you expert in what we are.</h2>
                                <p class="gr-text-8 mb-0">Now study UGC-NET Courses withut going anywhere in a price less then the fees of a coaching institute.</p>
                                <a href="#"
                                    class="btn-link with-icon gr-text-blue gr-text-7 font-weight-bold mt-9">Learn More<i class="icon icon-tail-right font-weight-bold"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="container">
                <div class="col-12 mb-8">
                    <h4>Choose your course</h4>
                </div>
                <div class="row mb-16 d-flex">
                <!-- <div class="main-carousel px-lg-12"
                    data-flickity='{ "cellAlign": "left", "contain": true, "pageDots": false }'> -->
                    <% courses.map(course=> { %>
                        <div class="carousel-cell col-sm-6 col-md-4 col-lg-3 my-6">
                            <%- include('components/course-card', course) %>
                        </div>
                        <% }) %>
                </div>
            </div>

            <div class="main-block pb-6 pb-lg-17 bg-default-6">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-4 col-lg-3">
                            <div class="rounded-10">
                                <div class="price-content light-mode-texts">
                                  <ul id="content-table" class="mx-3 pl-0 my-3 mx-auto">
                                    
                                  </ul>
                                </div>
                              </div>
                        </div>
                        <div class="col-xl-8 col-lg-9" id="article">
                           
                        </div>
                        </div>
                    </div>
            </div>




            <%- include('components/footer') %>
            <script>
                window.onload = () => {
                    $.ajax({
                        url: '/blog/content/60699c6bd34ceb2350211427', success: function (result) {

                            document.getElementById('article').innerHTML = result.content

                            var titles = $('#article h2')
                            var html = ''
                            Object.entries(titles).forEach((title, index) => {
                                title[1].id = "heading-" + index
                            })
                            Object.entries(titles).forEach((title, index) => {

                                if (title[1].innerText) {
                                    html += `<li class="text-black gr-text-9 mb-0 py-5 border-top border-bottom d-block">
                        <a href="#heading-${index}"><span class="font-weight-bold">${title[1].innerText}</span></a>
                        </li>`
                                }

                            })
                            document.getElementById('content-table').innerHTML = html
                        }
                    });

                }
            </script>
                </body>

</html>