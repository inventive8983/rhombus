<!-- Header Area -->
<div class="gr-bg-white-opacity-visible">
     <div class="container">
       <div class="row justify-content-between align-items-center">
         <!-- Brand Logo-->
            <div class="col-auto">
              <div class="brand-logo"><a href="/">
                <!-- light version logo (logo must be black)-->
                <img src="/static/image/logo-main-black.png" alt="" class="light-version-logo ">
                <!-- Dark version logo (logo must be White)-->
                <img src="/static/image/logo-main-white.png" alt="" class="dark-version-logo">
              </a></div>
            </div>
            <div class="ml-auto ml-lg-5 mr-6 mr-lg-0 d-none d-xs-flex align-items-center">
              <!-- <a class="btn btn-secondary gr-text-9 mx-3" href="/login">
                <i class="fa fa-key mr-2"></i>
                Log In
              </a> -->
              <% if(fundamentals.facebook){ %>
                <a href="<%= fundamentals.facebook %>" class="p-6 gr-text-8 text-dark gr-hover-text-blue">
                  <i class="fab fa-facebook-f "></i>
                </a>
                <% } %>
                <% if(fundamentals.twitter){ %>
                <a href="<%= fundamentals.twitter %>" class="p-6 gr-text-8 text-dark gr-hover-text-blue">
                  <i class="fab fa-twitter "></i>
                </a>
                <% } %>
                <% if(fundamentals.instagram){ %>
                <a href="<%= fundamentals.instagram %>" class="p-6 gr-text-8 text-dark gr-hover-text-blue">
                  <i class="fab fa-instagram "></i>
                </a>
                <% } %>
                <% if(fundamentals.linkedin){ %>
                <a href="<%= fundamentals.linkedin %>" class="p-6 gr-text-8 text-dark gr-hover-text-blue">
                  <i class="fab fa-linkedin "></i>
                </a>
                <% } %>
              <a class="header-btn btn btn-primary ml-10 gr-text-10" href="tel:<% if(fundamentals.mobile1){ %><%= fundamentals.mobile1 %><% } %>">
                <i class="fa fa-phone mr-2"></i>
                <span>Call Now</span> 
              </a>
            </div>
       </div>
     </div>
</div>
