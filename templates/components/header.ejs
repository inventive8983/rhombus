        <!--/.Mobile Menu Hamburger Ends-->
        <div class="d-flex align-items-center">
          <div class="ml-auto ml-lg-50 d-none d-xs-flex align-items-center justify-content-end">
              <% if(!authenticated){ %> 
                <a class="p-5 gr-text-9 text-black font-weight-bold mx-2 text-right gr-hover-text-blue" href="/login" role="button">Log In %></a>
                <!-- <a class="p-5 gr-text-9 text-black font-weight-bold mx-2 text-right gr-hover-text-blue" href="/comingsoon" role="button">Sign Up</a> -->
              <% }else{ %>
                <ul class="navbar-nav pl-lg-0 main-menu">
                  <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle gr-toggle-arrow" id="navbarDropdown02" href="/dashbaord" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><%= user.name%> <i class="icon icon-small-down"></i></a>
                    <ul class="gr-menu-dropdown dropdown-menu" aria-labelledby="navbarDropdown02">
                      <li class="drop-menu-item">
                        <a href="/dashboard">
