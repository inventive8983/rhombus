    <!-- Feature section -->
    <div class="feature-section py-2 bg-default-3">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-xl-6 col-lg-7 col-md-8">
            <div class="section-title text-center mb-13 mb-lg-23">
              <h4 class="pre-title gr-text-12 text-red text-uppercase mb-7">Why choose us</h4>
              <h2 class="title gr-text-4">People choose us because...</h2>
            </div>
          </div>
        </div>
        <div class="row justify-content-center">
          <div class="col-md-10">
            <div class="row align-items-center justify-content-center">
              <div class="col-lg-6 col-md-10 mb-11 mb-lg-19" data-aos="fade-left" data-aos-duration="800" data-aos-delay="200" data-aos-once="true">
                <div class="feature-widget media">
                  <div class="widget-icon p-7 mr-9">
                    <img src="/static/image/svg/conversation.svg" height="64px" alt="">
                  </div>
                  <div class="widget-text">
                    <h3 class="title gr-text-7 mb-6">Face to Face Classes</h3>
                    <p class="gr-text-9 mb-0 pr-11">we organise one to one meetings to gather the feedback from the students</p>
                  </div>
                </div>
              </div>
              <div class="col-lg-6 col-md-10 mb-11 mb-lg-19" data-aos="fade-left" data-aos-duration="800" data-aos-delay="400" data-aos-once="true">
                <div class="feature-widget media">
                  <div class="widget-icon p-7 mr-9">
                    <img src="/static/image/svg/books.svg" height="64px" alt="">
                  </div>
                  <div class="widget-text">
                    <h3 class="title gr-text-7 mb-6">Study Material & Assessments</h3>
                    <p class="gr-text-9 mb-0 pr-11">We  provide the precise and concise notes that are relative to the academics.</p>
                  </div>
                </div>
              </div>
              <div class="col-lg-6 col-md-10 mb-11 mb-lg-19" data-aos="fade-right" data-aos-duration="800" data-aos-delay="600" data-aos-once="true">
                <div class="feature-widget media">
                  <div class="widget-icon p-7 mr-9">
                    <img src="/static/image/svg/tour-guide.svg" height="64px" alt="">
                  </div>
                  <div class="widget-text">
                    <h3 class="title gr-text-7 mb-6">Notification Updates & Exam Guidance</h3>
                    <p class="gr-text-9 mb-0 pr-11">We provide you with one platform which gives you all the information about the exams</p>
                  </div>
                </div>
              </div>
              <div class="col-lg-6 col-md-10 mb-11 mb-lg-19" data-aos="fade-right" data-aos-duration="800" data-aos-delay="800" data-aos-once="true">
                <div class="feature-widget media">
                  <div class="widget-icon p-7 mr-9">
                    <img src="/static/image/svg/read.svg" height="64px" alt="">
                  </div>
                  <div class="widget-text">
                    <h3 class="title gr-text-7 mb-6">Smart Study Techniques</h3>
                    <p class="gr-text-9 mb-0 pr-11">We help the students to inculcate smart techniques which they can implement in their daily lifes as well.</p>
                  </div>
                </div>
              </div>
              <div class="col-lg-6 col-md-10 mb-11 mb-lg-19" data-aos="fade-right" data-aos-duration="800" data-aos-delay="800" data-aos-once="true">
                <div class="feature-widget media">
                  <div class="widget-icon p-7 mr-9">
                    <img src="/static/image/svg/presentation.svg" height="64px" alt="">
                  </div>
                  <div class="widget-text">
                    <h3 class="title gr-text-7 mb-6">Fully Featured Classrooms</h3>
                    <p class="gr-text-9 mb-0 pr-11">We are equipped with the best quality projectors and whiteboards</p>
                  </div>
                </div>
              </div>
              <div class="col-lg-6 col-md-10 mb-11 mb-lg-19" data-aos="fade-right" data-aos-duration="800" data-aos-delay="800" data-aos-once="true">
                <div class="feature-widget media">
                  <div class="widget-icon p-7 mr-9">
                    <img src="/static/image/svg/customer-service.svg" height="64px" alt="">
                  </div>
                  <div class="widget-text">
                    <h3 class="title gr-text-7 mb-6">24x7 Personalized Online Support</h3>
                    <p class="gr-text-9 mb-0 pr-11">Our 24x7 support is there to help out students in case of any issues</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="px-12 pt-16">
    <div class="row justify-content-center">
      <div class="col-xl-6 col-lg-7 col-md-8">
        <div class="section-title text-center">
          <h4 class="pre-title gr-text-12 text-red text-uppercase mb-7">Testimonials</h4>
          <h2 class="title gr-text-4">See what our students said...</h2>
        </div>
      </div>
    </div>
    <div class="py-lg-20 bg-whatsapp whatsapp-testimonials rounded-15 shadow-4 mt-8 mb-16">
        <div class="container">
          <div class="row">
            <% testimonials.forEach(data => { %>
            <div class="col-lg-10 offset-lg-1">
              <div class="d-flex my-6" data-aos="fade-up" data-aos-duration="800" data-aos-once="true">
                <div class="reviewer-img d-flex justify-content-center mb-7 mr-6 mb-lg-12">
                  <img class="img-fluid circle-sm" src="<%= data.avatar %>" alt="">
                </div>
                <div class="rounded-8 shadow-4 bg-whatsapp-chat px-8 py-6">
                    <span class="text-blue gr-text-9 font-weight-bold" ><%= data.name %></span><br/>
                    <%= data.reviews %>
                    <div class="text-right d-flex align-items-center justify-content-end mt-3">
                        <span>8:06PM</span>
                        <img src="/static/image/svg/double-tick-indicator.svg" class="ml-2" width="16px" height="16px">
                    </div>
                </div>
              </div>
            </div>
            <% }) %>
          </div>
        </div>
    </div>
  </div>
    <!-- CTA section 2 -->
    <div class="cta-section-2 dark-mode-texts bg-blackish-blue ">
      <div class="container">
        <div class="cta-wrapper pt-14 pb-14 py-lg-19 border-bottom">
          <div class="row align-items-center justify-content-center">
            <div class="col-lg-6 col-md-10">
              <div class="cta-text section-title">
                <h2 class="title gr-text-5 mb-7 text-white">Ready to prepare?</h2>
                <p class="gr-text-8 gr-color-white-opacity-7 mb-8 mb-lg-0"><% if(fundamentals.description){ %><%= fundamentals.description %><% } %></p>
              </div>
            </div>
            <div class="col-lg-4 offset-lg-2 col-md-10">
              <div class="cta-btn text-lg-right">
                <a href="#courses" class="btn btn-primary gr-hover-y">Get started on a course</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    
    <%- include('components/footer') %>

