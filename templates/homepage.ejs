    <div class="main-slider">
      <div class="main-carousel" data-flickity='{ "cellAlign": "left", "contain": true, "pageDots": true, "autoPlay": 7000, "pauseAutoPlayOnHover": false }'>
        <% slides.forEach(image => { %>
        <div class="carousel-cell w-100">
          <div class="w-100 bg-blue overflow-hidden">
            <img class="w-100" src="<%= image %>" alt="">
          </div>
        </div>
        <% }) %>
      </div>
    </div>

