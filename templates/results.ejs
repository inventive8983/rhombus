<!DOCTYPE html>
<html lang="en">
<%- include('components/head') %>
<%- include('components/theme') %>
<%- include('components/header') %>

<section style="background:rgb(17, 3, 43)" class="py-20 position-relative">
  <img src="/static/image/overlay.svg" style="mix-blend-mode: overlay;z-index: 0;position: absolute;top: 0px; left: 0px; width: 100%;height: 100%;object-fit: cover;">
    <div class="container">
      <div class="row">
        <div class="col-md-12 ml-n1 mt-12 mt-md-0">
          <div class="">
            <h3 id="course-name" class="gr-text-2 pt-5 text-white font-weight-bold">Our Results</h3>
          </div>
        </div>
      </div>
    </div>
  </section>
  <div class="section pb-20 bg-default-2">
    <div class="container ">
      <% files.forEach(collection => {%>
        <div class="row pt-5">
          <div class="text-center col-12 mt-12 mb-6">
            <h3><%= collection[0] %></h3>
          </div>
          <% collection[1].split(',').forEach(image => {%>
              <div class="col-8 offset-2">
                <img class="w-100 object-fit-cover" style="cursor: pointer;" data-fancybox="" href="<%= image %>" src="<%= image %>" alt="">
              </div>
            <% }) %>
        </div>
        <% }) %>
    </div>
  </div>
    
<%- include('components/footer') %>
</body>
</html>